<Window x:Class="LovesTestGitHub.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LovesTestGitHub"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Margin="50" Content="{Binding OutPut}"/>
        <TextBox VerticalAlignment="Center" Width="300" Text="{Binding InPut}"/>
        <Button VerticalAlignment="Bottom" Width="500" Margin="50" Background="Azure" Height="50" Content="Submitta" Command="{Binding ButtonCommand}"/>
    </Grid>
</Window>
